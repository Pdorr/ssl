#include <ctype.h>
#include <stdio.h>



int main (int argc, char *argv[]){
	in	= fopen(argv[1],"r");
	

	if(argc == 1){
		printf("\n(ERROR) Debe ingresar 1 nombre de archivo!\n");
		return -1;
	}
	if (argc != 2){
		printf("\n(ERROR) NÃºmero incorrecto de argumentos!\n");
		return -2;
	}
	if(in == NULL){
		printf("\n(File error) No se pudo abrir el archivo de Entrada.\n");
		return -3;
	}
	
	
	
	AnalizarArchivo(in);
	fclose(in);
	
	return 0;
}


void AnalizarArchivo (File * archivo){


}
int columna(char c){   //esto lo vamos a usar en lo que seria el automata para determinar si es lenguaje
    if(isalpha(c))  return 0;
    if(isdigit(c))  return 1;
    if(c =='+')     return 2;
    if(c =='-')     return 3;
    if(c =='(')     return 4;
    if(c ==')')     return 5;
    if(c ==',')     return 6;
    if(c ==';')     return 7;
    if(c ==':')     return 8;
    if(c =='=')     return 9;
    if(c == EOF)    return 10;
    if(isspace(c))  return 11;
    return 12;
}

