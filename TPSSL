#include <ctype.h>
#include <stdio.h>



int main (int argc, char *argv[]){
	in	= fopen(argv[1],"r");
	

	if(argc == 1){
		printf("\n(ERROR) Debe ingresar 1 nombre de archivo!\n");
		return -1;
	}
	if (argc != 2){
		printf("\n(ERROR) NÃºmero incorrecto de argumentos!\n");
		return -2;
	}
	if(in == NULL){
		printf("\n(File error) No se pudo abrir el archivo de Entrada.\n");
		return -3;
	}
	
	
	
	AnalizarArchivo(in);
	fclose(in);
	
	return 0;
}


